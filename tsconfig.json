{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@firstrankcoders/shared/*": ["./libs/shared/src/*"],
      "@firstrankcoders/shared/database": ["./libs/shared/src/database"],
      "@firstrankcoders/shared/utils": ["./libs/shared/src/utils"]
    },
    "moduleResolution": "node",
    "target": "es2019",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
