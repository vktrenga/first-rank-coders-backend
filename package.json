{
  "name": "first-rank-coders-backend",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "npm run build:libs && npm run build:apps",
    "build:libs": "npm run build -w @firstrankcoders/shared",
    "build:apps": "npm run build -w apps/user",
    "start:dev": "npm run start:dev -w apps/user",
    "start": "npm run start -w apps/user",
    "test": "npm run test -w apps/user",
    "clean": "rimraf \"{apps,libs}/**/dist\" \"{apps,libs}/**/node_modules\"",
    "install:all": "npm install && npm run build:libs"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@nestjs/config": "^4.0.2",
    "@prisma/client": "^6.19.0",
    "dotenv": "^17.2.3",
    "prisma": "^6.19.0"
  }
}
